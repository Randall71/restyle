  "dependencies": {},
  "devDependencies": {
    "@shopify/eslint-plugin": "^37.0.0",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^25.0.0",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.71.0",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^7.14.0",
    "husky": "^4.2.3",
    "jest": "^29.2.1",
    "prettier": "^1.18.2",
    "react": "^18.2.0",
    "react-native": "0.71.0",
    "react-test-renderer": "^18.2.0",
